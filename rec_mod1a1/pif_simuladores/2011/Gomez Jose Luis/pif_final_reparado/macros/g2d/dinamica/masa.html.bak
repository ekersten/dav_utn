   <param name="C_04" value="tipo='num&eacute;rico' id='go' interfaz='bot&oacute;n' regi&oacute;n='interior' espacio='E1' expresi&oacute;n='(300,360)' valor='1-go' nombre='stop-go' visible='si' acci&oacute;n='animar'">
